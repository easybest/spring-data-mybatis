<resultMap id="{{statementName}}" type="{{entityType}}">
    {{#results}}
        <{{#column}}{{#primaryKey}}id{{/primaryKey}}{{^primaryKey}}result{{/primaryKey}}{{/column}} property="{{name}}" column="{{columnName}}"
            {{#column}}
                {{#javaTypeString}}javaType="{{javaTypeString}}"{{/javaTypeString}}
                {{#jdbcTypeString}}jdbcType="{{jdbcTypeString}}"{{/jdbcTypeString}}
                {{#typeHandlerString}} typeHandler="{{typeHandlerString}}"{{/typeHandlerString}}
            {{/column}}
        />
    {{/results}}
    {{#embeddedAssociations}}
        <association property="{{key}}">
            {{#value}}
                <{{#column}}{{#primaryKey}}id{{/primaryKey}}{{^primaryKey}}result{{/primaryKey}}{{/column}} property="{{name}}" column="{{columnName}}"
                    {{#column}}
                        {{#javaTypeString}}javaType="{{javaTypeString}}"{{/javaTypeString}}
                        {{#jdbcTypeString}}jdbcType="{{jdbcTypeString}}"{{/jdbcTypeString}}
                        {{#typeHandlerString}} typeHandler="{{typeHandlerString}}"{{/typeHandlerString}}
                    {{/column}}
                />
            {{/value}}
        </association>
    {{/embeddedAssociations}}

    {{#associations}}
    <association property="{{property}}" javaType="{{javaType}}" fetchType="{{fetch}}" select="{{select}}" column="{{column}}"/>
    {{/associations}}
</resultMap>

