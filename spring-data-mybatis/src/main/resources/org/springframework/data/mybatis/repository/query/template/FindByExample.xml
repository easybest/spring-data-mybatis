<select id="{{statementName}}" parameterType="{{entityType}}Example" resultMap="{{resultMap}}">
    select
    <if test="distinct">distinct</if>
    <include refid="__base_column_list"/>
    from {{tableName}}
    <if test="_parameter != null">
        <include refid="__example_where_clause"/>
    </if>
    <if test="orderByClause != null">order by ${orderByClause}</if>
</select>