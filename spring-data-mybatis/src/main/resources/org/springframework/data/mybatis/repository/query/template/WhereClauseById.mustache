<sql id="{{ statementName }}">
    {{#domain}}
        {{#primaryKey}}
            {{#columns}}
                AND {{#quote}}{{alias}}{{/quote}}.{{name}} = {{#composited}}#{ {{property.name}}{{#javaType}},javaType={{javaTypeString}}{{/javaType}}{{#jdbcType}},jdbcType={{jdbcType.name}}{{/jdbcType}}{{#typeHandler}},typeHandler={{typeHandler.name}}{{/typeHandler}} }{{/composited}}
