<sql id="{{statementName}}">
    <trim suffixOverrides=",">
        {{#domain}}
            {{#primaryKey}}{{#columns}}{{#quote}}{{alias}}{{/quote}}.{{name}},{{/columns}}{{/primaryKey}}
            {{#columns}}{{#quote}}{{alias}}{{/quote}}.{{name}},{{/columns}}
            {{#embeddedDomains}}
                {{#columns}}{{#quote}}{{alias}}{{/quote}}.{{name}},{{/columns}}
            {{/embeddedDomains}}
            {{#oneToOneAssociations}}
                {{#leftJoin}}
                {{#columns}}{{#quote}}{{alias}}{{/quote}}.{{name}},{{/columns}}
                {{/leftJoin}}
            {{/oneToOneAssociations}}
            {{#manyToOneAssociations}}
                {{#leftJoin}}
                    {{#columns}}{{#quote}}{{alias}}{{/quote}}.{{name}},{{/columns}}
                {{/leftJoin}}
            {{/manyToOneAssociations}}
        {{/domain}}
    </trim>
</sql>
