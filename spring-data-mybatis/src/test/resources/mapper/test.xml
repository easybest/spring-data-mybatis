<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springframework.data.mybatis.repository.sample.EmployeeRepositoryWithEmbeddedId">
    <insert id="_insert" parameterType="org.springframework.data.mybatis.domain.sample.EmbeddedIdExampleEmployee"
            lang="XML"><![CDATA[
        insert into embedded_id_example_employee(name,department_id,employee_id) values (#{name,jdbcType=VARCHAR},#{org.springframework.data.mybatis.domain.sample.EmbeddedIdExampleEmployeePK.departmentId,jdbcType=BIGINT},#{org.springframework.data.mybatis.domain.sample.EmbeddedIdExampleEmployeePK.employeeId,jdbcType=BIGINT})
        ]]></insert>
</mapper>
