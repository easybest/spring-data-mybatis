<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %5p %40.40c:%4L - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="org.springframework.data" level="error"/>
    <logger name="org.springframework.data.mybatis.dialect" level="info"/>
    <!--<logger name="org.springframework.data.mybatis.repository.support" level="debug"/>-->
    <logger name="org.springframework.data.mybatis.repository.sample" level="debug"/>
    <!--<logger name="org.springframework.data.mybatis.repository.sample.UserRepository._insert" level="error"/>-->
    <!--<logger name="org.springframework.data.mybatis.repository.sample.UserRepository._insert!selectKey" level="error"/>-->
    <root level="error">
        <appender-ref ref="console"/>
    </root>

</configuration>