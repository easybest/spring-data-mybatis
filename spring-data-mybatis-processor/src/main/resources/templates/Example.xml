<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="{{metadata.repositoryClazzName}}">

  <sql id="Example_Where_Clause">
    <where>
      <foreach collection="oredCriteria" item="criteria" separator="or">
        <if test="criteria.valid">
          <trim prefix="(" prefixOverrides="and" suffix=")">
            <foreach collection="criteria.criteria" item="criterion">
              <choose>
                <when test="criterion.noValue">
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue">
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue">
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue">
                  and ${criterion.condition}
                  <foreach close=")" collection="criterion.value" item="listItem" open="(" separator=",">
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Update_By_Example_Where_Clause">
    <where>
      <foreach collection="example.oredCriteria" item="criteria" separator="or">
        <if test="criteria.valid">
          <trim prefix="(" prefixOverrides="and" suffix=")">
            <foreach collection="criteria.criteria" item="criterion">
              <choose>
                <when test="criterion.noValue">
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue">
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue">
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue">
                  and ${criterion.condition}
                  <foreach close=")" collection="criterion.value" item="listItem" open="(" separator=",">
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>

  <sql id="Base_Column_List">
    {{metadata.columns}}
  </sql>

  <sql id="TABLE_NAME">
    {{metadata.tableName}}
  </sql>

  <sql id="Table_Name_Dy">
    <if test="table==null">
      <include refid="TABLE_NAME" />
    </if>
    <if test="table!=null">
        ${table}
    </if>
  </sql>

  <sql id="Base_Column_List_Dy">
    <if test="columns==null">
      <include refid="Base_Column_List" />
    </if>
    <if test="columns!=null">
      <foreach collection="columns" item="column" separator=",">
        ${column}
      </foreach>
    </if>
  </sql>

  <select id="selectByExample" parameterType="{{metadata.exampleClazzName}}" resultMap="__result_map">
    select
    <if test="distinct">
      distinct
    </if>
    <include refid="Base_Column_List_Dy" />
    from
    <include refid="Table_Name_Dy" />
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null">
      order by ${orderByClause}
    </if>
    <if test="limit != null">
      limit
      <foreach collection="limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
  </select>
  <select id="streamByExample" parameterType="{{metadata.exampleClazzName}}" resultMap="__result_map">
    select
    <if test="distinct">
      distinct
    </if>
    <include refid="Base_Column_List_Dy" />
    from
    <include refid="Table_Name_Dy" />
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null">
      order by ${orderByClause}
    </if>
    <if test="limit != null">
      limit
      <foreach collection="limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
   </select>
  <select id="cursorByExample" parameterType="{{metadata.exampleClazzName}}" resultMap="__result_map">
    select
    <if test="distinct">
      distinct
    </if>
    <include refid="Base_Column_List_Dy" />
    from
    <include refid="Table_Name_Dy" />
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null">
      order by ${orderByClause}
    </if>
    <if test="limit != null">
      limit
      <foreach collection="limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
   </select>

  <delete id="deleteByExample" parameterType="{{metadata.exampleClazzName}}">
    delete from <include refid="Table_Name_Dy"></include>
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="limit != null">
      limit
      <foreach collection="limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
  </delete>

  <select id="countByExample" parameterType="{{metadata.exampleClazzName}}" resultType="long">
    select count(*) from <include refid="Table_Name_Dy"></include>
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
  </select>

  <update id="updateByExampleSelective" parameterType="map">
    update
    <if test="example.table==null">
      <include refid="TABLE_NAME" />
    </if>
    <if test="example.table!=null">
      ${example.table}
    </if>
    <set>
    {{#metadata.columnMetadataList}}
    <if test="record.{{fieldName}} != null">
      {{columnName}} = #{ record.{{fieldName}} } ,
    </if>
    {{/metadata.columnMetadataList}}
    </set>
    <if test="_parameter != null">
      <include refid="Update_By_Example_Where_Clause" />
    </if>
    <if test="example.limit != null">
      limit
      <foreach collection="example.limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
  </update>

  <update id="updateByExample" parameterType="map">
    update
    <if test="example.table==null">
      <include refid="TABLE_NAME" />
    </if>
    <if test="example.table!=null">
      ${example.table}
    </if>
    set
    <trim  suffixOverrides=",">
    {{#metadata.columnMetadataList}}
      {{columnName}} = #{ {{fieldName}} } ,
    {{/metadata.columnMetadataList}}
    </trim>
    <if test="_parameter != null">
      <include refid="Update_By_Example_Where_Clause" />
    </if>
    <if test="example.limit != null">
      limit
      <foreach collection="example.limit" item="num" separator=",">
        ${num}
      </foreach>
    </if>
  </update>

  <select id="existsByExample" parameterType="{{metadata.exampleClazzName}}" resultType="Boolean">
  select count(*)>0 from
    <include refid="Table_Name_Dy"></include>
      <if test="_parameter != null">
        <include refid="Example_Where_Clause"/>
      </if>
  </select>

  <select id="streamAll" resultMap="__result_map">
    select <include refid="Base_Column_List" /> from {{metadata.tableName}}
  </select>


  <select id="cursorAll" resultMap="__result_map">
    select <include refid="Base_Column_List" /> from {{metadata.tableName}}
  </select>

</mapper>
